  <Action th:each="entry : ${samplesForPairs}" th:with="sample=${entry.key},pair=${entry.value},forward=${pair.getForwardSequenceFile()},reverse=${pair.getReverseSequenceFile()}">
    <AddFiles target_db="SRA">
      <File file_path="_f1_R1.fastq_" th:attr="file_path=${forward.getFileName()}">
        <DataType>generic-data</DataType>
      </File>
      <File file_path="_f1_R2.fastq_" th:attr="file_path=${reverse.getFileName()}">>
        <DataType>generic-data</DataType>
      </File>
      <AttributeRefId name="experiment">
        <RefId>
          <SPUID spuid_namespace="CFSAN">CFSAN006236_01:2</SPUID>
        </RefId>
      </AttributeRefId>
      <Identifier>
        <SPUID spuid_namespace="CFSAN">CFSAN006236_01:3</SPUID>
      </Identifier>
    </AddFiles>
  </Action>

</Submission>
