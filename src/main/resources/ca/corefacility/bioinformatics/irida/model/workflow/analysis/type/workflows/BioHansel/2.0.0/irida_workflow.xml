<?xml version="1.0" encoding="UTF-8"?>
<iridaWorkflow>
  <id>0e8738e6-2aeb-4627-9d6e-15ae32f21c44</id>
  <name>biohansel</name>
  <version>2.1.5</version>
  <analysisType>BIOHANSEL</analysisType>
  <inputs>
    <sequenceReadsPaired>sequence_reads_paired</sequenceReadsPaired>
    <requiresSingleSample>true</requiresSingleSample>
  </inputs>
  <parameters>
    <parameter name="biohansel-1-type_of_scheme.scheme_type" required="true">
      <choices>
        <choice name="heidelberg" value="heidelberg"/>
        <choice name="enteritidis" value="enteritidis"/>
      </choices>
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="type_of_scheme.scheme_type"/>
    </parameter>
    <parameter name="biohansel-1-kmer_vals.kmer_min" defaultValue="8">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="kmer_vals.kmer_min"/>
    </parameter>
    <parameter name="biohansel-1-kmer_vals.kmer_max" defaultValue="1000">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="kmer_vals.kmer_max"/>
    </parameter>
    <parameter name="biohansel-1-qc_vals.max_missing_tiles" defaultValue="0.05">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="qc_vals.max_missing_tiles"/>
    </parameter>
    <parameter name="biohansel-1-qc_vals.low_coverage_warning" defaultValue="20">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="qc_vals.low_coverage_warning"/>
    </parameter>
    <parameter name="biohansel-1-qc_vals.low_cov_depth_freq" defaultValue="20">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="qc_vals.low_cov_depth_freq"/>
    </parameter>
    <parameter name="biohansel-1-qc_vals.min_ambiguous_tiles" defaultValue="3">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="qc_vals.min_ambiguous_tiles"/>
    </parameter>
    <parameter name="biohansel-1-qc_vals.max_intermediate_tiles" defaultValue="0.05">
      <toolParameter toolId="toolshed.g2.bx.psu.edu/repos/nml/biohansel/biohansel/2.1.5" parameterName="qc_vals.max_intermediate_tiles"/>
    </parameter>
  </parameters>
  <outputs>
    <output name="biohansel-match_results.json" fileName="biohansel-match_results.json"/>
    <output name="biohansel-match_results.tab" fileName="biohansel-match_results.tab"/>
    <output name="biohansel-results.json" fileName="biohansel-results.json"/>
    <output name="biohansel-results.tab" fileName="biohansel-results.tab"/>
    <output name="biohansel-tech_results.json" fileName="biohansel-tech_results.json"/>
    <output name="biohansel-tech_results.tab" fileName="biohansel-tech_results.tab"/>
  </outputs>
  <toolRepositories>
    <repository>
      <name>biohansel</name>
      <owner>nml</owner>
      <url>https://toolshed.g2.bx.psu.edu</url>
      <revision>ba6a0af656a6</revision>
    </repository>
  </toolRepositories>
</iridaWorkflow>

