        </repository>
        <repository>
            <name>bundle_collections</name>
            <owner>irida</owner>
            <url>https://irida.corefacility.ca/galaxy-shed</url>
            <revision>7bc329e1ada4</revision>
        </repository>
        <repository>
            <name>prokka</name>
            <owner>crs4</owner>
            <url>https://toolshed.g2.bx.psu.edu</url>
            <revision>eaee459f3d69</revision>
        </repository>
        <repository>
            <name>quast</name>
            <owner>iuc</owner>
            <url>https://toolshed.g2.bx.psu.edu</url>
            <revision>0834c823d4b9</revision>
        </repository>
    </toolRepositories>
</iridaWorkflow>
